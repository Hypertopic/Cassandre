<?xml version="1.0" encoding="UTF-8"?>
<diary name="{{diary_name}}">
{{#sections}}
  {{#memos}}
  <memo id="{{id}}" type="{{type}}" author="{{author}}" created="{{date}}" update="{{update}}">
    <groundings>
      {{#groundings}}
      <grounding id="{{id}}">{{name}}</grounding>
      {{/groundings}}
    </groundings>
    {{#preview}}<content>{{preview}}</content>{{/preview}}
  </memo>
  {{/memos}}
{{/sections}}
</diary>

