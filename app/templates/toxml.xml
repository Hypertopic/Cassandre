<?xml version="1.0" encoding="UTF-8"?>
<diary name="{{diary_name}}">
  {{#memos}}
  <memo id="{{id}}" type="{{type}}" author="{{author}}" created="{{date}}" {{#update}} update="{{update}}"{{/update}}>
    <groundings>
      {{#groundings}}
      <grounding id="{{id}}">{{name}}</grounding>
      {{/groundings}}
    </groundings>
    {{#body}}<content>{{body}}</content>{{/body}}
  </memo>
  {{/memos}}
</diary>

